{
    "name": "symm/viewranger-api-client",
    "description": "Client for the ViewRanger API",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Gareth Jones",
            "email": "me@gazj.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "4.*",
        "guzzlehttp/command": "~0.4",
        "guzzlehttp/guzzle-services": "~0.1",
        "doctrine/collections": "~1.0",
        "whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {"Symm\\ViewRangerClient": "src/"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
